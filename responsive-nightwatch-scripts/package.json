    "bs_all": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e ie_11_win_8_1,ie_11_win_7,ie_10_win_7,ie_9_win_7,ie_8_win_7,chrome_win_10,chrome_Win_8,chrome_Win_7,safari_mac,firefox_win_10,firefox_win_7,ie_edge_windows_10,chrome_android_tablet,chrome_android_S_6,safari_ios_iphone_5s,safari_ios_iphone_6,safari_ios_ipad_air",
    "bs_ie_11_win_8_1": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e ie_11_win_8_1",
    "bs_ie_11_win_7": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e ie_11_win_7",
    "bs_ie_10_win_7": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e ie_10_win_7",
    "bs_ie_9_win_7": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e ie_9_win_7",
    "bs_ie_8_win_7": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e ie_8_win_7",
    "bs_chrome_win_10": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e chrome_win_10",
    "bs_chrome_Win_8": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e chrome_Win_8",
    "bs_chrome_Win_7": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e chrome_Win_7",
    "bs_safari_mac": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e safari_mac",
    "bs_firefox_win_10": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e firefox_win_10",
    "bs_firefox_win_7": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e firefox_win_7",
    "bs_ie_edge_windows_10": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e ie_edge_windows_10",
    "bs_chrome_android_tablet": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e chrome_android_tablet",
    "bs_chrome_android_S_6": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e chrome_android_S_6",
    "bs_safari_ios_iphone_5s": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e safari_ios_iphone_5s",
