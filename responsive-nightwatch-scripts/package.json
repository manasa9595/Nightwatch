    "bs_safari_ios_ipad_air": "node browserstack.runner.js -c nightwatch_browserstack.conf.js -e safari_ios_ipad_air",
    "zeplin": "node nightwatch.js tests/shelfs/zeplinTest.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.digital.rbsgrp.mde/Prustis/responsive-nightwatch-scripts.git"
  },
