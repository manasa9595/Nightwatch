    "async": "^2.6.1",
    "browserstack-local": "^1.3.0",
    "grunt": "^1.0.3",
    "grunt-cli": "^1.2.0",
    "js-beautify": "^1.7.5",
    "nightwatch": "^0.8.18",
    "node-run-cmd": "^1.0.1",
